<div class="container">
  <div class="row my-3">
    <div class="col-lg-12 i-show-empty">
      <h4 class="pr-3 pt-3">会場登録</h4>
      <%= form_with model:@artist, url: admins_artists_path, local:true do |f| %>
      <table class="table">
          <tr>
            <th class="">名前</th>
            <td><%= f.text_field :name %></td>
          </tr>
            
        </table>
        
        <%= f.submit "新規登録", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>