<div class="container">
  <div class="row my-3">
    <div class="col-lg-12 i-show-empty">
      <h4 class="pr-3 pt-3">会場登録</h4>
      <%= form_with model:@venue, url: admins_venues_path, local:true do |f| %>
      <table class="table">
          <tr>
            <th class="">会場写真</th>
            <td><%= f.attachment_field :image %></td>
          </tr>
          <tr>
            <th class="">会場名</th>
            <td><%= f.text_field :name %></td>
          </tr>
          <tr>
            <th class="">郵便番号</th>
            <td><%= f.text_field :postcode %></td>
          </tr>
           <tr>
            <th class="">住所</th>
            <td><%= f.text_area :address %></td>
          </tr>
          <tr>
            <th class="">都道府県</th>
            <td><div class="field">
              <%= f.select :prefecture, Venue.prefectures.keys, {prompt: '選択してください'}, class: 'form-control btn btn-info' %>
            </div></td>
          </tr>
            
        </table>
        
        <%= f.submit "新規登録", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>