<div class="container">
  <div clsss="row my-3">
      <div class="my-3 h4">アーティスト一覧</div>
    <table class="table">
      <thead class="thead-light">
        <tr>
          <th>名前</th>
        </tr>
      </thead>
      <tbody>
        <% @artists.each do |artist| %>
          <tr>
            <td>
              <%= link_to artist_path(artist) do %>
                <%= artist.name %>
              <% end %>
            </td>
            
          </tr>
        <% end %>
      </tbody>
    </table>
    <div class="paginate mb-3">
      <%= paginate @artists %>
    </div>
  </div>
</div>